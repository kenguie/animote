<!DOCTYPE html>
<html>
<head>
  <title>Animote</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <style><%= yield :stylesheets %></style> 
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %> 
  <script type="text/javascript" src="/assets/javascripts/bootstrap_js/bootstrap.min.js"></script>
  <script type="text/javascript" src="/assets/javascripts/jquery.slides.js"></script>

  <link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css" />
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
    
  <%= csrf_meta_tags %> 

</head>
<body>

  <header class="main-nav">
    <%= render '/users/shared/nav' %>
  </header> 
  

  <% if notice %><div id="notice"><%= notice %></div><% end %>
  <% if alert %><div id="alert"><%= alert %></div><% end %>

  <%= yield %>

</body>
</html>
